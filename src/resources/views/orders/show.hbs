<div  role="main">
            <div class="">
                <div class="page-title">
                    <div class="title_left">
                        	<a href="/orders/show?s=delivering" class="btn btn-secondary">Delivering</a>
                            <a href="/orders/show?s=delivered" class="btn btn-secondary">Delivered</a>
                            <a href="/orders/show?s=not" class="btn btn-secondary">Not delivery</a>
                </div>
                <div class="clearfix"></div>

                <div class="row">
                    <div class="col-md-12">
                        <div class="x_panel">

                            <div class="x_content">



                                <!-- start project list -->
                                <table class="table table-striped projects">
                                    <thead>
                                        <tr>
                                            <th style="width: 5%">#</th>
                                            <th style="width: 20%">Name customer</th>
                                            <th style="width: 30%">Address</th>
                                            <th style="width: 15%">Status</th>
                                            <th style="width: 30%">Order date</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {{#each input}}
                                        <tr>
                                            <td>#</td>
                                            <td>
                                                <a>{{this._name}}</a>

                                            </td>
                                            <td>
                                                <a>{{this._address}}</a>
                                            </td>
                                            <td>
                                                <a>{{this._status}}</a>
                                            </td>
                                            <td>
                                                <a>{{this._book}}</a>
                                            </td>
                                        </tr>
                                        {{/each}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            </div>
