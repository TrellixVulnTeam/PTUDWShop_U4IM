<div  role="main">
            <div class="">
                <div class="page-title">
                    	<h3>Top 10 products by quantity</h3>
                    <div class="title_left">
                        
                </div>
                <div class="clearfix"></div>

                <div class="row">
                    <div class="col-md-12">
                        <div class="x_panel">

                            <div class="x_content">



                                <!-- start project list -->
                                <table class="table table-striped projects">
                                    <thead>
                                        <tr>
                                            <th style="width: 5%">#</th>
                                            <th style="width: 50%">Name</th>
                                            <th style="width: 10%">Procedure</th>
                                            <th style="width: 5%">Amount</th>
                                            <th style="width: 10%">Price</th>
                                            <th style="width: 10%">Turnover</th>
                                            <th style="width: 10%">Detail</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {{#each statisticals}}
                                        <tr>
                                            <td>#</td>
                                            <td>
                                                <a>{{this.name}}</a>

                                            </td>
                                            <td>
                                                <a>{{this.procedure}}</a>
                                            </td>
                                            <td>
                                                <a>{{this.amount}}</a>
                                            </td>
                                            <td>
                                                <a>{{this.price}}</a>
                                            </td>
                                          <td>
                                                <a>{{this.turnover}}</a>
                                            </td>
                                            <td>
                                                <a href="/products/{{this.id}}/update">detail</a>
                                            </td>
                                        </tr>
                                        {{/each}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            </div>
